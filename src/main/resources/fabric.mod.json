{
  "schemaVersion": 1,
  "id": "apron",
  "version": "${version}",
  "name": "Apron",
  "description": "A compatibility layer for legacy API mods.\n- Risugami's ModLoader\n- Risugami's ModLoaderMP\n- Minecraft Forge\n- AudioMod\n- ShockAhPI",
  "authors": [
    {
      "name": "Better Than Updates",
      "contact": {
        "sources": "https://github.com/Better-Than-Updates-MC/babricated-forge",
        "issues": "https://github.com/Better-Than-Updates-MC/babricated-forge/issues"
      }
    }
  ],
  "contributors": [
    {
      "name": "CatCore",
      "contact": {
        "sources": "https://github.com/arthurbambou/Fabricated-Legacy-Forge",
        "issues": "https://github.com/arthurbambou/Fabricated-Legacy-Forge/issues"
      }
    },
    {
      "name": "halotroop2288",
      "contact": {
        "email": "carolinejoybell@gmail.com"
      }
    }
  ],
  "contact": {
    "homepage": "https://better-than-updates.github.io/",
    "sources": "https://github.com/Better-Than-Updates-MC/babricated-forge",
    "issues": "https://github.com/Better-Than-Updates-MC/babricated-forge/issues"
  },
  "license": "MIT",
  "icon": "assets/apron/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "io.github.betterthanupdates.apron.impl.client.ApronClientImpl"
    ],
    "server": [
      "io.github.betterthanupdates.apron.impl.server.ApronServerImpl"
    ],
    "mod-remapper-api:modremapper": [
      "io.github.betterthanupdates.apron.ApronModRemapper"
    ]
  },
  "accessWidener": "apron.accesswidener",
  "mixins": [
    "apron.mixins.json",
    "audiomod.mixins.json",
    "forge.mixins.json",
    "modloader.mixins.json",
    "shockahpi.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13.3",
    "minecraft": "1.0.0-beta.7.3",
    "mod-remapping-api": "*"
  },
  "conflicts": {
    "modloader": "<2.0.0"
  }
}
