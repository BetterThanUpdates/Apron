{
  "schemaVersion": 1,
  "id": "babricated-forge",
  "version": "${version}",
  "name": "Babricated Forge",
  "description": "A compatibility layer to load Forge mods with Fabric.",
  "authors": [
    {
      "name": "CatCore",
      "contact": {
        "sources": "https://github.com/arthurbambou/Fabricated-Legacy-Forge",
        "issues": "https://github.com/arthurbambou/Fabricated-Legacy-Forge/issues"
      }
    },
    {
      "name": "halotroop2288",
      "contact": {
        "email": "carolinejoybell@gmail.com"
      }
    }
  ],
  "contact": {
    "homepage": "https://better-than-updates.github.io/",
    "sources": "https://github.com/Better-Than-Updates-MC/babricated-forge",
    "issues": "https://github.com/Better-Than-Updates-MC/babricated-forge/issues"
  },
  "license": "MIT",
  "icon": "assets/babricated-forge/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "io.github.betterthanupdates.babricated.impl.client.ClientUtil"
    ],
    "server": [
      "io.github.betterthanupdates.babricated.impl.server.ServerUtil"
    ],
    "mod-remapper-api:modremapper": [
      "io.github.betterthanupdates.babricated.BabricatedModRemapper"
    ]
  },
  "accessWidener": "forge.accesswidener",
  "mixins": [
    "audiomod.mixins.json",
    "babricated.mixins.json",
    "forge.mixins.json",
    "modloader.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13.3",
    "minecraft": "1.0.0-beta.7.3",
    "mod-remapping-api": "*"
  }
}
